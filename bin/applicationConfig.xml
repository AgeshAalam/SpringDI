<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<!-- SETTER INJECTION -->
<bean id="emp" class="org.kms.spring.Employee">
	<property name="id" value="12345"></property>
	<property name="name" value="MUNI"></property>
</bean>
<!-- CONSTRUCTOR INJECTION -->
<bean id="emp2" class="org.kms.spring.Employee">
	<constructor-arg type="int" value="23"></constructor-arg>
	<constructor-arg type="String" value="AGESH"></constructor-arg>
</bean>
	
</beans>